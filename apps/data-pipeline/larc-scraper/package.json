{
  "name": "@apps/larc-scraper",
  "version": "0.0.0",
  "private": true,
  "description": "Scraper for LARC data",
  "type": "module",
  "scripts": {
    "check:types": "tsc -p ./tsconfig.json -noEmit",
    "deploy": "wrangler deploy --minify src/handler.ts",
    "dev": "dotenv -e ../../../.env -- wrangler dev --test-scheduled src/handler.ts",
    "postinstall": "wrangler types --x-include-runtime",
    "start": "dotenv -e ../../../.env -- tsx src/index.ts"
  },
  "dependencies": {
    "@packages/db": "workspace:*",
    "@packages/stdlib": "workspace:*",
    "cheerio": "1.0.0",
    "cross-fetch": "4.0.0"
  },
  "devDependencies": {
    "@types/node": "20.8.3",
    "wrangler": "3.84.1"
  }
}
